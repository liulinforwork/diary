<import src="../../wxParse/wxParse.wxml"/>
<view class="wrap">
     <view class="top">
        <image src="{{info.introduce}}" alt="图片加载失败" class="img"></image>
     </view>
     <view class="info">
        <view class="title">{{info.title}}</view>
        <view class="text">{{info.info}}</view>
     </view>

      <view class="footer" catchtap="toGuide">
        <image src="../../images/invite.png" class="invite"></image>
        <text class="btn">报名活动</text>
      </view>

     <view class="content">
        <template is="wxParse" data="{{wxParseData:article.nodes}}"/>
     </view>


</view>